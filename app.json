{
  "expo": {
    "name": "movieApp",
    "slug": "movieApp",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true,
      "requireFullScreen": true
    },
    "build": {
      "production": {
        "android": {
          "buildType": "apk",
          "adaptiveIcon": {
            "foregroundImage": "./assets/adaptive-icon.png",
            "backgroundColor": "#ffffff"
          }
        }
      }
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "extra": {
      "eas": {
        "projectId": "b2caba87-f809-4581-b9ee-a8a267dbb289"
      }
    },
    "android": {
      "softwareKeyboardLayoutMode": "pan",
      "package": "com.wajoj80758.movieApp"
    },
    "plugins": [
      "expo-font",
      "expo-asset"
    ]
  }
}
